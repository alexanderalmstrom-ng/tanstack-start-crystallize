{
  "name": "tanstack-start-shopify",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "pnpm codegen && vite build",
    "build:local": "pnpm codegen:local && vite build",
    "start": "vite preview",
    "test": "vitest run",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "codegen": "npm-run-all --parallel codegen:catalogue codegen:discovery",
    "codegen:catalogue": "dotenvx run --env-file .env -- graphql-codegen --config codegen.catalogue.ts",
    "codegen:discovery": "dotenvx run --env-file .env -- graphql-codegen --config codegen.discovery.ts",
    "codegen:watch": "npm-run-all --parallel codegen:catalogue:watch codegen:discovery:watch",
    "codegen:catalogue:watch": "dotenvx run --env-file .env -- graphql-codegen --config codegen.catalogue.ts --watch",
    "codegen:discovery:watch": "dotenvx run --env-file .env -- graphql-codegen --config codegen.discovery.ts --watch"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@shopify/hydrogen-react": "^2025.7.0",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-devtools": "^0.8.1",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-query-devtools": "^5.91.0",
    "@tanstack/react-router": "^1.139.3",
    "@tanstack/react-router-devtools": "^1.139.3",
    "@tanstack/react-router-ssr-query": "^1.139.3",
    "@tanstack/react-start": "^1.139.3",
    "@tanstack/router-plugin": "^1.139.3",
    "@trpc/client": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "@trpc/tanstack-react-query": "^11.7.2",
    "@unpic/react": "^1.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.12.0",
    "lucide-react": "^0.544.0",
    "nitro": "latest",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.66.1",
    "server-only": "^0.0.1",
    "superjson": "^2.2.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.15.1",
    "@biomejs/biome": "2.2.4",
    "@graphql-codegen/cli": "^6.0.2",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript": "^5.0.5",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.5.1",
    "@tanstack/devtools-vite": "^0.3.11",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.10"
  }
}